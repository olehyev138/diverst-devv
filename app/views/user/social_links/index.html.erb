<div class="content__header">
  <h1>My Social Media Posts</h1>
  <%= link_to "Add", new_group_path, class: "btn btn--primary" %>
</div>

<div class="content__main">
  <div class="card">
    <% @posts.each do |post| %>
      <div class="card__section">
        <%= raw post.embed_code %>
      </div>
    <% end %>
  </div>
</div>