<div id="embedded-feed" style="height:800px;width:400px;"></div>
<script type="text/javascript" src="https://s0.assets-yammer.com/assets/platform_embed.js" id='yammer_script'></script>
<script type="text/javascript">
  $("#yammer_script").load(function(){
    function init_yammer() {
      yam.connect.embedFeed({
        container: "#embedded-feed",
        feedType: "group",
        feedId: "<%= @group.yammer_group_id %>"
      });
    }

    setTimeout(init_yammer, 2000);
  });
</script>
