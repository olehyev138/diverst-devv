<div id="calendar"></div>

<script>
  $(document).ready(function(){
    $('#calendar').fullCalendar({
      header: {
        left:   'title',
        center: 'month,agendaWeek',
        right:  'prev,today,next'
      },
      eventSources: [
        {
          url: '<%= calendar_data_group_events_path(@group) %>',
          color: BRANDING_COLOR,
          textColor: 'white'
        }
      ]
    }); //endof fullcalendar
  }); //endof document.ready
</script>