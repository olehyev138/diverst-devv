<h2 class="h1-styling">Twilio Dashboard</h2>

<div class="card">
    <div class="card__section">
      <h3 class="h3-styling">Virtual Events Stats</h3>

      <table 
        id="twilio-usage-datatable"
        data-no-sortable='3'
        class="table--has-border data-table" 
        data-source="<%= twilio_dashboard_index_path(format: :json) %>">
        <thead>
        <th>Room/Event Name</th>
        <th>Start Time</th>
        <th>End Time</th>
        <th>Duration</th>
        <th>Participant No.</th>
        <th>Billing</th>
        </thead>
        <tbody></tbody>
      </table>
    </div>
  </div>