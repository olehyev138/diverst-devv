<div class="content__main">
  <div class="row">

    <div class="col sm-12 md-12">
      <h2 class="h1-styling">Add custom texts to your app</h2>
      <div class="card">
        <div class="card__section">
          <%= simple_form_for @custom_text do |f| %>
            <% CustomText.keys.each do |key| %>
              <%= f.input key, as: :string %>
            <% end %>
            <%= f.button :submit %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
