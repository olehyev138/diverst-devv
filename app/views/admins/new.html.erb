<div class="content__header">
  <h1>
    <%= link_to "Admins", admins_path %> /
    Create an admin
  </h1>
</div>
<div class="content__main">
  <div class="card">
    <div class="card__section">
      <%= simple_form_for @admin do |f| %>
        <%= f.input :first_name %>
        <%= f.input :last_name %>
        <%= f.input :email %>
        <%= f.input :password %>
        <%= f.input :password_confirmation %>

        <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>