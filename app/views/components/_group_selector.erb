<div class="modal group-selector" tabindex="-1" role="dialog" id="<%= id %>" data-url="<%= get_paginated_groups_groups_path %>" data-multiselect="<%= multiselect %>">
    <div class="modal-dialog" role="document">
        <div class="modal-content card">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="groups-content modal-body">
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col pull-right">
                        <button type="button" class="btn btn--primary btn--small save-groups-btn">Save</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>