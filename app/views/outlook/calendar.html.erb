<h1>My events</h1>
<table class="table">
  <tr>
    <th>Subject</th>
    <th>Start</th>
    <th>End</th>
  </tr>
  <% @events.each do |event| %>
    <tr>
      <td><%= event.subject %></td>
      <td><%= event.start.date_time %></td>
      <td><%= event.end.date_time %></td>
    </tr>
  <% end %>
</table>